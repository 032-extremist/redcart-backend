MPESA_CALLBACK_BASE_URL=
MPESA_TRANSACTION_TYPE=CustomerPayBillOnline
SMTP_ENABLED=false
SMTP_HOST=
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=
SMTP_PASS=
SMTP_FROM=
